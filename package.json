{
  "name": "inpaint-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "localforage": "^1.10.0",
    "opencv-ts": "^1.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.1",
    "react-zoom-pan-pinch": "^3.3.0"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "dev": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/index.css --watch",
    "react-scripts:start": "delay 5 && react-scripts start",
    "react-scripts:build": "react-scripts build"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,css,sass,scss,md}": [
      "prettier --write",
      "echo '统一格式化完成🌸'"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "autoprefixer": "10.x",
    "cross-env": "7.x",
    "delay-cli": "^1.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "4.x",
    "postcss": "8.x",
    "postcss-cli": "8.x",
    "postcss-preset-env": "6.x",
    "prettier": "^2.4.1",
    "react-scripts": "^5.0.1",
    "tailwind-scrollbar": "^2.0.0",
    "tailwindcss": "2.x",
    "typescript": "4.x"
  }
}
